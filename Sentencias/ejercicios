num1 = 10
num2 = 3
num3= 5
# 1
if num1 > num2 or num2 > num1 or num3 == num1:
    print("si es verdadero ")
else:
    print("es falso")

# 2
edad1 = 10
edad2 = 20
edad3=5

if edad1 > edad2 and edad2 > edad1 or edad3 == edad1:
    print("es verdadero")
else:
    print("es falso")

#3
nota1=10
