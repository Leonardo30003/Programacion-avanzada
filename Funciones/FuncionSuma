def sumaSinParametros():
    return 2+2
print(sumaSinParametros())


def sumaConParametros(num1,num2):
    return num1 + num2
print(sumaConParametros(2,5))


def restaSinParametros():
    return 1-3
print(restaSinParametros())


def restaConParametros(num1,num2):
    return num1 - num2
print(restaConParametros(5,9))
