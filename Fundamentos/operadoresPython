# listas

lista = ["Juan", "Pedro", "Martha"]
print(type(lista))
print(lista[2])
lista[2] = "Juana"  # remplazar
lista.append("Diego")  # agregar
print(lista)
lista2 = lista.copy()
lista2.clear()
print(lista2)
